</title>\n      </Helmet>\n      <h1>Featured Products</h1>\n      <div className=\"products\">\n        {loading ? (\n          <LoadingBox />\n        ) : error ? (\n          <MessageBox variant=\"danger\">{error}</MessageBox>\n        ) : (\n          <Row>\n            {products.map((product) => (\n              <Col key={product.slug} sm={6} md={4} lg={3} className=\"mb-3\">\n                <Product product={product}></Product>\n              </Col>\n            ))}\n          </Row>\n        )}\n      </div>\n    </div>\n  );\n}\nexport default HomeScreen;\n","/home/gaurav/REACT/mern-amazona/frontend/src/components/Product.js",[],"/home/gaurav/REACT/mern-amazona/frontend/src/components/Rating.js",[],"/home/gaurav/REACT/mern-amazona/frontend/src/utils.js",[],"/home/gaurav/REACT/mern-amazona/frontend/src/components/MessageBox.js",[],"/home/gaurav/REACT/mern-amazona/frontend/src/components/LoadingBox.js",[],"/home/gaurav/REACT/mern-amazona/frontend/src/Store.js",[],"/home/gaurav/REACT/mern-amazona/frontend/src/screens/CartScreen.js",[],"/home/gaurav/REACT/mern-amazona/frontend/src/screens/SigninScreen.js",[],"/home/gaurav/REACT/mern-amazona/frontend/src/screens/SignupScreen.js",[],"/home/gaurav/REACT/mern-amazona/frontend/src/screens/ShippingAddressScreen.js",[],"/home/gaurav/REACT/mern-amazona/frontend/src/components/CheckoutSteps.js",[],"/home/gaurav/REACT/mern-amazona/frontend/src/screens/PaymentMethodScreen.js",[],"/home/gaurav/REACT/mern-amazona/frontend/src/screens/PlaceOrderScreen.js",[],"/home/gaurav/REACT/mern-amazona/frontend/src/screens/OrderScreen.js",[],"/home/gaurav/REACT/mern-amazona/frontend/src/screens/OrderHistoryScreen.js",[],"/home/gaurav/REACT/mern-amazona/frontend/src/screens/ProfileScreen.js",["93","94"],"import React, { useContext, useReducer, useState } from 'react';\nimport { Helmet } from 'react-helmet-async';\nimport Form from 'react-bootstrap/Form';\nimport Button from 'react-bootstrap/Button';\nimport { Store } from '../Store';\nim